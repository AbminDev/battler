    "dialogues": {
      "dialogue1": {
        "person": "1",
        "nextStep": "/tutorial/dungeon",
        "phrases": [
          {
            "person": "",
            "me": "It seems there's someone ahead..."
          },
          {
            "person": "",
            "me": "What are you doing here?"
          },
          {
            "person": "My name is Lira. I got lost in these caves trying to find treasures.",
            "me": "It seems I'm lost too."
          },
          {
            "person": "There’s an exit ahead, but we need to defeat a monster.",
            "me": "…"
          },
          {
            "person": "Will you help me?",
            "me": "Alright, let's go together. But be careful."
          },
          {
            "person": "I'm sure we can handle it together.",
            "me": "Move forward"
          }
        ]
      },
      "dialogue2": {
        "person": "1",
        "nextStep": "/battle",
        "phrases": [
          {
            "person": "",
            "me": "Phew, that wasn't easy. Good thing you were here."
          },
          {
            "person": "Well done. Now we have everything we need.",
            "me": "What do you mean?"
          },
          {
            "person": "(laughing) Did you really think I was lost?",
            "me": "I don’t understand…"
          },
          {
            "person": "How naive. I needed someone to clear the monsters out of the way.",
            "me": "What?! You used me!"
          },
          {
            "person": "Of course. And now, farewell. I hope you're not too disappointed.",
            "me": "…"
          }
        ]
      },
      "dialogue3": {
        "person": "2",
        "nextStep": "/",
        "phrases": [
          {
            "person": "",
            "me": "Where am I? Who are you?"
          },
          {
            "person": "I am the Goddess Kitsune. I have resurrected you for a great mission.",
            "me": "Why me?"
          },
          {
            "person": "You were chosen for your wisdom and strength.",
            "me": "Continued listen"
          },
          {
            "person": "Our world is in ruins, and only you can restore its greatness.",
            "me": "What must I do?"
          },
          {
            "person": "Build a city, unite the survivors.",
            "me": "I think it’s not all business."
          },
          {
            "person": "Find ancient relics to awaken the sleeping powers of the world.",
            "me": "It sounds like a difficult task, but I’m ready."
          },
          {
            "person": "Don't forget: there are many enemies and secrets around.",
            "me": "Thank you for the second chance. I will succeed."
          }
        ]
      }
    }
